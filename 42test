#basic commands

  
ls
ls -l -a
ls -la
ls -l -- nosuchfile
  ls    -la   -r   
pwd
cat
/bin/ls
/bin/ls -a -l
/bin/nosuchcmd

#separators

ls;ls
ls ; ls -a;
ls;;ls
ls; ;ls
;
ls;ls;ls;ls

#pipes
|
|l
ls |
ls | cat
ls | wc -l
ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls | ls
base64 /dev/urandom | head -c 1000 | grep 'AUCUNECHANCEQUECETTESTRINGSOITTROUVEELOOOL'

#redirections

>
> /tmp/shtest.log
/bin/echo lol>/dev/null
/bin/echo lol1>   /dev/null
/bin/echo lol 1>   /dev/null
/bin/echo lol 1> &  /dev/null
/bin/echo lol 1>&/dev/null
/bin/echo 'Clafouti' > /tmp/merlin.log
cat < /tmp/merlin.log > /tmp/elias.log; cat /tmp/elias.log
/bin/echo 123 > /tmp/shtest.log; /bin/echo 456 >> /tmp/shtest.log; cat /tmp/shtest.log



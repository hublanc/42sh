#builtin cd

cd; pwd
cd     
cd /tmp; pwd
cd nosuchdir
cd /tmp; touch foo; cd foo
cd /tmp; mkdir bar; chmod a-x bar; cd bar; rm -rf bar
cd -; pwd
cd /tmp; cd -; pwd
CDPATH=toto; cd /tmp; cd test
CDPATH=test; cd /tmp; cd lol
CDPATH=toto; cd /tmp; mkdir toto; mkdir /tmp/toto/test; cd test
CDPATH= ; cd / ; cd tmp

#builtin env

env
env -q
env -i
env ls
env nosuchfile
env SWAGG_VAR=SWAGG_VAL | sort
env -i SWAGG_VAR=SWAGG_VAL
env -i SWAGG_VAR==
env -i cd
env -i cd -

#builtin setenv

<custom>setenv SWAGG_VAR SWAGG_VAL
/bin/echo -n
<custom>setenv SWAGG_VAR SWAGG_VAL; env | sort
env SWAGG_VAR=SWAGG_VAL | sort
<custom>setenv SWAGG_VAR; env | sort
env SWAGG_VAR= | sort
<custom>setenv SWAGG_VAR OUI; setenv SWAGG_VAR NON; env | sort
env SWAGG_VAR=NON | sort

#builtin unsetenv
